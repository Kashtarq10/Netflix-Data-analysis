  {
   "cell_type": "code",
   "execution_count": 452,
   "id": "ffcc6d9a-a8b0-4117-8bfd-42a34ed9f7af",
   "metadata": {},
   "outputs": [],
   "source": [
    "# CATEGORIZE THE AVERAGE VOTE_AVERAGE COLUMN AS PER \" (dataset, column and add lables)\n",
    "# USER_DEFINE_FUNCTION\n",
    "#\n",
    "def categorize_col(df, col, labels):\n",
    "    edges = [\n",
    "        df[col].describe()['min'],\n",
    "        df[col].describe()['25%'],\n",
    "        df[col].describe()['50%'],\n",
    "        df[col].describe()['75%'],\n",
    "        df[col].describe()['max']\n",
    "    ]\n",
    "\n",
    "    df[col] = pd.cut(df[col], edges, labels=labels, duplicates='drop')\n",
    "    return df\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 454,
   "id": "64e1c05b-3ef1-4501-8b39-2989859fb88d",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['popular', 'belowaverage', 'average', 'notPopular', NaN]\n",
       "Categories (4, object): ['notPopular' < 'belowaverage' < 'average' < 'popular']"
      ]
     },
     "execution_count": 454,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "labels = ['notPopular', 'belowaverage', 'average', 'popular']\n",
    "categorize_col(df, 'Vote_Average', labels)\n",
    "df['Vote_Average'].unique()\n",
    "\n"
   ]
